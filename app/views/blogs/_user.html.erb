<div>
<h3>Users</h3>
<% User.all.limit(2).each do |user| %>
    <p><strong> Name:</strong>  <%= user.name%></p>
    <p><strong> Nickname:</strong> <%= user.nickname%></p>
    <p><strong> Address:</strong> <%= user.address%></p>
    <p><strong> Created_at:</strong> <%= user.created_at%></p>
<% end %>
</div>