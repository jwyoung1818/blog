<div>
<h3>Blogs</h3>
<% @blogs.each do |blog| %>
<div class="card">
    <p>
    <strong>Title:</strong>
    <%= blog.title %>
    </p>
    <p>
    <strong>created_at:</strong>
    <%= blog.created_at %>
    </p>
    
    <p>
    <strong>Text:</strong>
    <%= blog.text %>
    </p>
</div>
<% end %>
<%= will_paginate @blogs %>
</div>
